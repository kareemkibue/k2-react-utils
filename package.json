{
	"name": "k2-react-utils",
	"version": "0.12.0",
	"description": "A collection of handy React/JavaScript utilities",
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/kareemkibue/k2-react-utils.git"
	},
	"author": "Kareem Kibue <mail@kareemkibue.com>",
	"license": "MIT",
	"scripts": {
		"start": "npm run tsc:watch",
		"tsc:watch": "tsc -w",
		"clean": "del-cli -f node_modules && del-cli -f yarn.lock && yarn",
		"build": "del-cli -f .publish && tsc",
		"npm:publish": "npm run build && npm test && npm run copy:packagejson && cd .publish && npm publish",
		"copy:packagejson": "copy package.json .publish\\",
		"copy:npmignore": "copy .npmignore .publish\\",
		"test": "node_modules/.bin/jest --coverage --passWithNoTests",
		"test:watch": "node_modules/.bin/jest --watchAll",
		"postpublish": "echo 'k2-React published successfully to NPM.' "
	},
	"dependencies": {
		"xml-js": "^1.6.1"
	},
	"peerDependencies": {
		"react": "*"
	},
	"devDependencies": {
		"@types/jest": "24.0.9",
		"@types/react": "16.8.6",
		"@types/react-redux": "7.0.1",
		"@types/react-test-renderer": "16.8.1",
		"del-cli": "1.1.0",
		"jest": "24.1.0",
		"jest-dom": "3.1.2",
		"jest-extended": "0.11.1",
		"npm-run-all": "4.1.5",
		"react": "16.8.3",
		"react-dom": "16.8.3",
		"react-redux": "6.0.1",
		"react-test-renderer": "16.8.3",
		"react-testing-library": "6.0.0",
		"redux": "4.0.1",
		"ts-jest": "24.0.0",
		"typescript": "3.3.3333"
	}
}
